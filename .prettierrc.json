{
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "printWidth": 140,
  "bracketSameLine": false,
  "trailingComma": "all",
  "arrowParens": "always",
  "singleAttributePerLine": false,
  "endOfLine": "lf",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": ["<TYPES>", "", "<BUILTIN_MODULES>", "", "<THIRD_PARTY_MODULES>", "", "^~/(.*)$", "", "<TYPES>^[.]", "", "^[./]"],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
